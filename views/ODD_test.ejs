<h1>hackathon_DB_Parge_log_test</h1>
<p> Welcome to ODD DB test page</p>
<p> Record_Start_Time for test: <%= data[0]['Record_Finish_Time'] %> </p>

<table id="odd_table" class="table table-bordered table-hover table-striped">
    <thead>
    <tr>
        <th>NO.</th>
        <th>Task Name</th>
        <th>HTML Status Code</th>
        <th>Record Size (Bytes)</th>
        <th>Record Start Time</th>
        <th>Record Finish Time</th>
        <th>Record Download Duration (Seconds)</th>
    </tr>
    </thead>
    <tbody>

    <% data.forEach(function(row,index){ %>
    <tr>
        <th><%= index+1 %></th>
        <th><%= row['Task_Name']  %></th>
        <th><%= row['HTML_Status_Code'] %></th>
        <th><%= row['Record_Size_(Bytes)'] %></th>
        <th><%= row['Record_Start_Time'] %></th>
        <th><%= row['Record_Finish_Time'] %></th>
        <th><%= row['Task_Download_Frequency_(Seconds)'] %></th>
    </tr>
    <% }); %>

